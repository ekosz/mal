{
  "name": "mal",
  "private": true,
  "version": "1.0.0",
  "description": "Make a Lisp (mal) language implemented in Rescript",
  "type": "module",
  "scripts": {
    "build": "rescript",
    "watch": "rescript build -w",
    "test": "npm run build && npm run test:step0 && npm run test:step1 && npm run test:step2 && npm run test:step3 && npm run test:step4 && npm run test:step5 && npm run test:step6 && npm run test:step7 && npm run test:step8 && npm run test:step9 && npm run test:stepA",
    "test:step0": "cd ../.. && make 'test^rescript^step0'",
    "test:step1": "cd ../.. && make 'test^rescript^step1'",
    "test:step2": "cd ../.. && make 'test^rescript^step2'",
    "test:step3": "cd ../.. && make 'test^rescript^step3'",
    "test:step4": "cd ../.. && make 'test^rescript^step4'",
    "test:step5": "cd ../.. && make 'test^rescript^step5'",
    "test:step6": "cd ../.. && make 'test^rescript^step6'",
    "test:step7": "cd ../.. && make 'test^rescript^step7'",
    "test:step8": "cd ../.. && make 'test^rescript^step8'",
    "test:step9": "cd ../.. && make 'test^rescript^step9'",
    "test:stepA": "cd ../.. && make 'test^rescript^stepA'"
  },
  "dependencies": {},
  "devDependencies": {
    "rescript": "^9.1.4"
  }
}
